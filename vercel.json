{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "framework": "nextjs",
  "env": [
    {
      "key": "HF_TOKEN",
      "scope": "development"
    },
    {
      "key": "NEXT_PUBLIC_API_URL",
      "value": "http://localhost:8000",
      "scope": "development"
    }
  ]
}
